@model IEnumerable<The_NewYork_Time.Models.Article>

@foreach (var item in Model)
{
                <div class="row paper-border-top-light">
                    <div class="col-md">
                        <div class="paper ">
                            <div class="paper-body">
                                <h5 class="title-paper"><a class="text-decoration-none text-dark" href="">@item.articlename</a></h5>
                                <p class="paper-item-abstract"><a class="text-decoration-none text-black-50" href="#">@item.description.</a>
                            </div>
                        </div>
                    </div>
                    <div class="col-md">
                        <div class="box" style="min-width: 100%; max-height: 100%;">
                            <img style="width:100%;height:100%;" src="@item.image" alt="Alternate Text" />
                        </div>
                    </div>
                </div>

}
