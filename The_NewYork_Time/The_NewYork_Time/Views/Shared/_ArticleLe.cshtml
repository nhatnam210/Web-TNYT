@model IEnumerable<The_NewYork_Time.Models.Article>

@foreach (var item in Model)
{
<div class="row">
    <div class="col-md paper-border-top-light">
        <div class="paper ">
            <div class="paper-body">
                <h5 class="title-paper"><a class="text-decoration-none text-dark" href="@Url.Action("Article","Categories" ,new { id =item.idarticle})">@item.articlename</a></h5>
                <p class="paper-item-abstract text-decoration-none text-black-50">@item.description</p>
            </div>
        </div>
    </div>
</div>
}
