@model IEnumerable<The_NewYork_Time.Models.Article>
<ul class="list-content">
    @foreach (var item in Model)
    {
        <li>
            
            <a href="/Categories/Article/@item.Id">
                <img src="@item.image" alt="">
            </a>
            <h2>
                <a href="/Categories/Article/@item.Id">@item.description</a>
            </h2>
        </li>
    }

   

</ul>
