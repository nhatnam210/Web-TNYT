
@model IEnumerable<The_NewYork_Time.Models.Category>
<ul class="footer-col-ul list-group list-group-flush" style="list-style:none;">
    @foreach (var item in Model)
    {
        <li class="footer-category__item"><a href="@Url.Action("Category","Sections",new {id =item.Id })" class="footer-category__link">@item.categoryname</a></li>
    }


</ul>

